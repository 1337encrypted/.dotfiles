# -----------------------------------------------------
# Exports
# -----------------------------------------------------
export EDITOR=nvim
export PATH="/usr/lib/ccache/bin/:$PATH"
export ZSH="$HOME/.oh-my-zsh"

# Check if . is already in PATH
if [[ ":$PATH:" != *":.:"* ]]; then
    export PATH="$PATH:."
fi

